<html manifest="http://fs.sports.naver.com:<%= clientPort %>/game/m/<%= version %>/offline.appcache" style="padding: 0px; margin: 0px;">



<head>
    <title>판타지스타디움</title>
    <meta charset="UTF-8" />
    <base href="http://fs.sports.naver.com:<%= clientPort %>/game/m/<%=version%>/">
    <meta http-equiv="X-UA-Compatible" content="IE=Edge,chrome=1" />


    <link rel="manifest" href="http://fs.sports.naver.com:<%= clientPort %>/game/m/<%= version %>/appmanifest.json" />


    <style type="text/css">
        body { background-color: black; color: white; }
        canvas { -ms-touch-action: none; }
        .game-bg-splash {
            background: url('<%=webImgURL%>/assets/images/mobile-sheet0.png')  no-repeat ;
            margin: 0px;
            padding: 0px;
        }
        .game_base{padding:0; position:relative; width:1200px; height:750px; margin:0 auto;}
        .browser_update_game { display:block; position:absolute; left:480px; top:30px; width:690px; height:690px; background:url("<%=webImgURL%>/assets/images/game_browserPop.png") no-repeat; text-align: center; z-index: 3; }
        .browser_update_game em{font-style:normal;}
        .browser_update_game h1,h2,h3 {display:none;}
        .browser_update_game .check { margin-top:160px; margin-bottom: 5px; font-size:18px; font-weight: bold; color: #132c39; }
        .browser_update_game .check em { color: #e60012; font-weight: bold; }
        .browser_update_game .txt { margin-bottom: 65px; font-size: 14px; line-height: 22px; color: #4e78aa; font-weight:bold;}
        .browser_update_game ul { overflow: hidden; text-align: center; margin-bottom:35px; position:relative; left:66px; }
        .browser_update_game ul li { display: inline-block; margin-left:30px; float:left; }
        .browser_update_game ul li:first-child { margin: 0; }
        .browser_update_game ul li img, .browser_update_game ul li strong { display: block; }
        .browser_update_game ul li img { margin-bottom:5px;}
        .browser_update_game ul li a img{padding-left:24px;}
        .browser_update_game ul li strong { margin-bottom:5px; color:#0a427c; font-weight:bold; font-size:13px;}
        .browser_update_game .sub_txt { margin-bottom:20px; margin-left:45px; font-size:12px; line-height: 18px; text-align:left; color:#4e78aa; font-weight:bold; }
        .browser_update_game .sub_txt em { color: #132c39; font-weight:bold; }
        .browser_update_game .btns { padding-top:10px; }
    </style>

    <style>
        body, div, ul, li, dl, dt, dd, ol, p, h1, h2, h3, h4, h5, h6, form, section {margin: 0;	padding: 0;}
        li {list-style-type: none;}
        img, fieldset, iframe {border: 0;}
        img {border: none; vertical-align: top;}
        input {vertical-align: middle; -moz-box-sizing:border-box; -webkit-box-sizing: border-box; box-sizing:border-box;}
        a {text-decoration: none; color:#808080;}
        em {font-style: normal;}
        textarea:focus, input:focus{outline: 0;}
        .clear {clear: both;}
        body, th, td, input, select, textarea, button { font-size: 12px; line-height: 1.5; font-family: 'Nanum Gothic', '맑은 고딕', 'Malgun Gothic', sans-serif; color: #666; }


        .hidden{font:0px/0 a; height:0; overflow:hidden; position:absolute; visibility:hidden; width:0;}
        #wrap{position:relative; width:100%; height:auto; max-width:720px; top:0; margin:0 auto; padding:0;  background:#fff;}
    </style>


    <script>

        var gameOptions ={'gameURL':'/game/' + <%= version %> + '/index.html','isIE':<%=IE%>} ;
    </script>
    <link rel="shortcut icon" href="<%=webImgURL%>/favicon/favicon_16.ico" type="image/x-icon">
    <link rel="apple-touch-icon" href="<%=webImgURL%>/favicon/favicon64.png" />
</head>
